
public class TestArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

//		int[] a = new int[]{ 17, 19, 23, 29, 31, 37 };

		int a[3]= {0,1,2);
		int[] b = a;
		for (int i : b) {
			System.out.println(i);
		}
		
		a[3]=0;
		System.out.println("---------------");
		for (int i : b) {
			System.out.println(i);
		}
	}

}
